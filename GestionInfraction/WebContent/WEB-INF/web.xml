<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>GestionInfraction</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>VIEWS/Home/index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <error-page>
   	<error-code>400</error-code>
   	<location>/VIEWS/Erreur/erreurPage400.jsp</location>
  </error-page>
  
  <error-page>
   	<error-code>401</error-code>
   	<location>/VIEWS/Erreur/erreurPage401.jsp</location>
  </error-page>
  
  <error-page>
   	<error-code>403</error-code>
   	<location>/VIEWS/Erreur/erreurPage403.jsp</location>
  </error-page>
  
  <error-page>
   	<error-code>404</error-code>
   	<location>/VIEWS/Erreur/erreurPage404.jsp</location>
  </error-page>
  
  <error-page>
   	<error-code>405</error-code>
   	<location>/VIEWS/Erreur/erreurPage405.jsp</location>
  </error-page>
  
  <error-page>
   	<error-code>500</error-code>
   	<location>/VIEWS/Erreur/erreurPage500.jsp</location>
  </error-page> 
   
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
</web-app>